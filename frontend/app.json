        "hdpi":"./assets/splash-screens/android/drawable-hdpi/splash.png",
        "xhdpi":"./assets/splash-screens/android/drawable-xhdpi/splash.png",
        "xxhdpi":"./assets/splash-screens/android/drawable-xxhdpi/splash.png",
        "xxxhdpi":"./assets/splash-screens/android/drawable-xxxhdpi/splash.png",
        "backgroundColor":"#FDE101"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
